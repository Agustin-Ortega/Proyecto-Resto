@model Proyecto_Resto.Models.ItemReserva

@{
    ViewData["Title"] = "Delete";
}



<h3>Esta seguro/a que quiere eliminar este plato del menu?</h3>
<div style="background-color:black; color:antiquewhite">
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Plato)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Plato.descricpion)
        </dd>
    </dl>
    
    <div align="center">
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Quitar" class="btn btn-outline-danger my-3" /> |
            <a asp-action="Index">volver</a>
        </form>
    </div>
    
</div>
